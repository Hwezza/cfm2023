def test_MM():
    import gui
    guibrain = gui.guibrain()
    guibrain.open_menu()
    guibrain.mainwindow
    assert type(guibrain.main_window)==tkinter.Tk , "good"

    guibrain.open_menu()
    guibrain.firstoption
    assert type(guibrain.first_option)==tkinter.Tk, "good"

    guibrain.show_data()
    guibrain.selectedparticle
    assert selected_particle() == mainBrain.particle_list[number], "nice"